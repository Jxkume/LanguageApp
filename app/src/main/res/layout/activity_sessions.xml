<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="#FAF3EC"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/session1_btn"
        android:layout_width="252dp"
        android:layout_height="72dp"
        android:layout_marginTop="70dp"
        android:layout_marginBottom="15dp"
        android:contextClickable="false"
        app:layout_constraintBottom_toTopOf="@+id/session2_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.127"
        app:srcCompat="@drawable/newsession_btn" />

    <ImageView
        android:id="@+id/session2_btn"
        android:layout_width="252dp"
        android:layout_height="72dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toTopOf="@+id/session3_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/session1_btn"
        app:srcCompat="@drawable/newsession_btn" />

    <ImageView
        android:id="@+id/session3_btn"
        android:layout_width="252dp"
        android:layout_height="72dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toTopOf="@+id/session4_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/session2_btn"
        app:srcCompat="@drawable/newsession_btn" />

    <ImageView
        android:id="@+id/session4_btn"
        android:layout_width="252dp"
        android:layout_height="72dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toTopOf="@+id/session5_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/session3_btn"
        app:srcCompat="@drawable/newsession_btn" />

    <ImageView
        android:id="@+id/session5_btn"
        android:layout_width="252dp"
        android:layout_height="72dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/session4_btn"
        app:srcCompat="@drawable/newsession_btn" />

    <TextView
        android:id="@+id/firstSessionUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/baloo2_extra"
        android:gravity="center"
        android:textColor="@color/font"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/session1_btn"
        app:layout_constraintEnd_toEndOf="@+id/session1_btn"
        app:layout_constraintStart_toStartOf="@+id/session1_btn"
        app:layout_constraintTop_toTopOf="@+id/session1_btn" />

    <TextView
        android:id="@+id/secondSessionUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/baloo2_extra"
        android:gravity="center"
        android:textColor="@color/font"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/session2_btn"
        app:layout_constraintEnd_toEndOf="@+id/session2_btn"
        app:layout_constraintStart_toStartOf="@+id/session2_btn"
        app:layout_constraintTop_toTopOf="@+id/session2_btn" />

    <TextView
        android:id="@+id/thirdSessionUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/baloo2_extra"
        android:gravity="center"
        android:textColor="@color/font"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/session3_btn"
        app:layout_constraintEnd_toEndOf="@+id/session3_btn"
        app:layout_constraintStart_toStartOf="@+id/session3_btn"
        app:layout_constraintTop_toTopOf="@+id/session3_btn" />

    <TextView
        android:id="@+id/fourthSessionUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/baloo2_extra"
        android:gravity="center"
        android:textColor="@color/font"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/session4_btn"
        app:layout_constraintEnd_toEndOf="@+id/session4_btn"
        app:layout_constraintStart_toStartOf="@+id/session4_btn"
        app:layout_constraintTop_toTopOf="@+id/session4_btn" />

    <TextView
        android:id="@+id/fifthSessionUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/baloo2_extra"
        android:gravity="center"
        android:textColor="@color/font"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/session5_btn"
        app:layout_constraintEnd_toEndOf="@+id/session5_btn"
        app:layout_constraintStart_toStartOf="@+id/session5_btn"
        app:layout_constraintTop_toTopOf="@+id/session5_btn" />

</androidx.constraintlayout.widget.ConstraintLayout>